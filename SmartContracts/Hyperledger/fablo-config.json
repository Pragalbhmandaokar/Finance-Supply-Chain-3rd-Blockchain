{
    "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.1.0/schema.json",
    "global": {
        "fabricVersion": "2.4.0",
        "tls": false,
        "peerDevMode": false,
        "explorer": true
    },
    "orgs": [{
            "organization": {
                "name": "supplychainorderer",
                "domain": "supplychainorderer.supplychain.com"
            },
            "orderers": [{
                "groupName": "group1",
                "type": "solo",
                "instances": 1
            }],
            "tools": {
                "fabloRest": false
            }
        },
        {
            "organization": {
                "name": "Company",
                "domain": "company.supplychain.com"
            },
            "peer": {
                "instances": 1,
                "db": "CouchDb"
            },
            "tools": {
                "fabloRest": false
            }
        },
        {
            "organization": {
                "name": "Financial",
                "domain": "financial.supplychain.com"
            },
            "peer": {
                "instances": 1,
                "db": "CouchDb"
            },
            "tools": {
                "fabloRest": false
            }
        }
    ],
    "channels": [{
        "name": "financesupplychain",
        "orgs": [{
                "name": "Company",
                "peers": ["peer0"]
            },
            {
                "name": "Financial",
                "peers": ["peer0"]
            }
        ]
    }],
    "chaincodes": [{
        "name": "financeChaincode",
        "version": "1.0",
        "directory": "./chaincode",
        "lang": "node",
        "channel": "financesupplychain"
    }]
}